<div class="container mt-3">
  <h1>Movie Lists</h1>
  <% @lists.each do |list| %>
    <ul>
      <li><%= link_to "#{list.name}", list_path(list), class: 'text-decoration-none' %></li>
    </ul>
  <% end %>
  <%= link_to "Create a list", new_list_path, class: 'btn btn-secondary text-decoration-none mt-3 mb-3' %>
</div>
